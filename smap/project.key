\javaSource ".";

\schemaVariables{\term String s; \term Object t;}
\rules {
       equm4strdirect {
                      \find(equm(s,t))
                      \replacewith(   t != null & s != null
                                    & java.lang.String::instance(t) = TRUE
                                    & strContent(s) = strContent((java.lang.String)t))
                    \heuristics(simplify)
       };
}

\chooseContract
